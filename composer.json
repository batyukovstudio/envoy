{
    "name": "batyukovstudio/envoy",
    "description": "Envoy Container for apiato.",
    "keywords": [
        "envoy",
        "git",
        "laravel",
        "apiato"
    ],
    "authors": [
        {
            "name": "batyukovstudio.com",
            "email": "batyukovstudio@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Batyukovstudio\\Envoy\\": "src/"
        }
    },
    "require": {
        "php": ">=8.2",
        "laravel/framework": "^10.0",
        "laravel/envoy": "^2.9",
        "spatie/laravel-github-webhooks": "^1.2"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Batyukovstudio\\Envoy\\EnvoyServiceProvider"
            ]
        }
    },
    "scripts": {
        "post-update-cmd": [
            "php -r \"copy(__DIR__.'/Envoy.blade.php', './Envoy.blade.php');\""
        ]
    }
}
